<tr>
  <td> <%= link_to image_tag(group.avatar.url(:large)), group_path(group), :target => "_blank" %></td>
  <td><%= link_to(group.title, group_path(group)) %></td>
  <td><%= render_group_description(group) %></td>
  <td><%= group.group_relationships.count %></td>
  <td><%= group.posts.count %></td>
  <td> <%= group.user.username %> </td>
</tr>
